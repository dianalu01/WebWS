<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.Admazing">
    <class name="com.admazing.ZonaComercialModel" table="zonacomercial" >
        <id name="idzonacomercial" type="string">
            <column name="idzonacomercial" length="8" not-null="true"/>
        </id>
        <property name="nombre" type="string" column="nombre" length="40"/>
        <property name="longitud" type="string" column="longitud" length="10" />
        <property name="latitud" type="string" column="latitud" length="10" />
        <property name="distancia" type="string" column="distancia" length="10" />
	</class>
</hibernate-mapping>