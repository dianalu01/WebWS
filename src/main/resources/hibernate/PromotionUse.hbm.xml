<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.Admazing">
<class name="com.admazing.PromotionUseModel" table="usopromocion" >
        <id name="idPromotionUse" type="string">
            <column name="idusopromocion" length="10" not-null="true"/>
        </id>
        <property name="idUser" type="string" column="idusuario" length="8"  not-null="true"/>
        <property name="idPromotion" type="string" column="idpromocion" length="10" not-null="true"/>
	</class>
</hibernate-mapping>
