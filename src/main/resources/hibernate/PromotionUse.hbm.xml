<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.Admazing">
<class name="com.admazing.PromotionUseModel" table="usopromocion" >
        <id name="idUse" type="string">
            <column name="codigouso" length="8" not-null="true"/>
        </id>
        <property name="idAccess" type="string" column="codigoacceso" length="10"  not-null="true"/>
        <property name="idPromotion" type="string" column="promocion" length="10" not-null="true"/>
	</class>
</hibernate-mapping>