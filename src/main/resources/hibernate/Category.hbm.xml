<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.Admazing">
    <class name="com.admazing.CategoriaModel" table="categoria" >
        <id name="idCategory" type="string">
            <column name="idcategoria" length="8" not-null="true"/>
        </id>
        <property name="description" type="string" column="descripcion" length="30"/>
	</class>
</hibernate-mapping>