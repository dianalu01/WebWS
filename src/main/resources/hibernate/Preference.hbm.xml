<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.Admazing">
<class name="com.admazing.PreferenceModel" table="preferencia" >
        <id name="idPreference" type="string">
            <column name="idpreferencia" length="8" not-null="true"/>
        </id>
        <property name="idUser" type="string" column="idusuario" length="8"  not-null="true"/>
        <property name="idCategory" type="string" column="idcategoria" length="8" not-null="true"/>
	</class>
</hibernate-mapping>