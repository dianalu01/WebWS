<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.Admazing">
    <class name="com.admazing.CouponBookModel" table="cuponera" >
        <composite-id >
        	<key-property name="idUser" type="string" column="idusuario" length="8"/> 
        	<key-property name="idCouponBook" type="string" column="idcuponera" length="8"/> 
        </composite-id>
        <property name="idPromotion" type="string" column="promocion" length="10"/>
	</class>
</hibernate-mapping>