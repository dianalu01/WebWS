<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.Admazing">
    <class name="com.admazing.StoreCategoryModel" table="tiendacategoria" >
        <id name="idStoreCategory" type="string">
            <column name="codigotc" length="8" not-null="true"/>
        </id>
        <property name="idStore" type="string" column="codtienda" length="8"/>
        <property name="idCategory" type="string" column="codcategoria" length="8"/>
	</class>
</hibernate-mapping>